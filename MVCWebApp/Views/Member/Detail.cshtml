@{
}

<h2>Member Details</h2>

<table class="table">
    <tr>
        <td>Id</td>
        <td>@ViewBag.Member.Id</td>
    </tr>
    <tr>
        <td>Name</td>
        <td>@ViewBag.Member.Name</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>@ViewBag.Member.Email</td>
    </tr>
    <tr>
        <td>Active</td>
        <td>@(ViewBag.Member.Active ? "Active" : "Inactive")</td>
    </tr>
</table>